{
	"name": "kent-socialising-bot",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "@sapphire",
	"license": "UNLICENSE",
	"type": "module",
	"imports": {
		"#root/*": [
			"./dist/*.js"
		],
		"#lib/*": [
			"./dist/lib/*.js"
		]
	},
	"dependencies": {
		"@prisma/client": "^5.6.0",
		"@sapphire/decorators": "^6.0.2",
		"@sapphire/discord.js-utilities": "7.1.2",
		"@sapphire/duration": "^1.1.0",
		"@sapphire/eslint-plugin-result": "^2.0.1",
		"@sapphire/framework": "^5.2.1",
		"@sapphire/plugin-hmr": "^2.0.3",
		"@sapphire/plugin-logger": "^3.0.7",
		"@sapphire/plugin-subcommands": "^5.1.0",
		"@sapphire/plugin-utilities-store": "^1.0.3",
		"@sapphire/utilities": "^3.13.0",
		"@skyra/env-utilities": "^1.2.1",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"colorette": "^2.0.20",
		"discord.js": "^14.14.1",
		"lint-staged": "^15.1.0",
		"pretty-quick": "^3.1.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@sapphire/cli": "^1.8.0",
		"@sapphire/eslint-config": "^5.0.2",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.0",
		"@types/eslint": "^8.44.7",
		"@types/node": "^20.10.0",
		"@types/ws": "^8.5.10",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"husky": "^8.0.3",
		"prettier": "^3.1.0",
		"prisma": "^5.6.0",
		"tsc-watch": "^6.0.4",
		"typescript": "^5.3.2"
	},
	"scripts": {
		"sapphire": "sapphire",
		"generate": "sapphire generate",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"start": "node dist/index.js",
		"dev": "npm run build && npm run start",
		"watch:start": "tsc-watch -b src --onSuccess \"npm run start\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint src --ext ts --fix",
		"postinstall": "yarn prisma generate"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@4.0.2"
}
